{
    "env": {
        "es2020": true,
        "node": true
    },
    "extends": [
        "google"
    ],
    "parserOptions": {
        "ecmaVersion": 11,
        "sourceType": "module"
    },
    "rules": {
        "quotes": [2, "double", { "avoidEscape": true }],
        "max-len": "off",
        "require-jsdoc": "off",
        "valid-jsdoc": "off",
        "no-tabs": "off",
        "brace-style": "off"
    }
}
